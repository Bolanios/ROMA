                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/icons8_adelante_24" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".12"
            android:layout_marginBottom="@dimen/marginbottom_menu"
            android:background="@drawable/ripple_effect"
            android:clickable="true"
            android:focusable="true"
            android:onClick="goDiabetes"
            android:orientation="horizontal"
            android:paddingHorizontal="@dimen/paddinghorizontal_menu"
            android:paddingVertical="@dimen/paddingvertical_menu">

            <ImageView
                android:id="@+id/iconSound6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/padding_buttom"
                android:layout_toStartOf="@id/adelante6"
                android:contentDescription="@string/diabetes"
                android:onClick="sound"
                app:srcCompat="@drawable/icons8_altavoz_24" />

            <ImageView
                android:id="@+id/imageViewConsulta6"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/iconodiabetes1" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/paddingViews"
                android:layout_toEndOf="@+id/imageViewConsulta6"
                android:text="@string/diabetes" />

            <ImageView
                android:id="@+id/adelante6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/icons8_adelante_24" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".12"
            android:layout_marginBottom="@dimen/marginbottom_menu"
            android:clickable="true"
            android:focusable="true"
            android:background="@drawable/ripple_effect"
            android:onClick="goObesidad"
            android:orientation="horizontal"
            android:paddingHorizontal="@dimen/paddinghorizontal_menu"
            android:paddingVertical="@dimen/paddingvertical_menu">

            <ImageView
                android:id="@+id/iconSound7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/padding_buttom"
                android:layout_toStartOf="@id/adelante7"
                android:contentDescription="@string/obesidad"
                app:srcCompat="@drawable/icons8_altavoz_24"
                android:onClick="sound"/>

            <ImageView
                android:id="@+id/imageViewConsulta7"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/iconoobesidad1" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/paddingViews"
                android:layout_toEndOf="@+id/imageViewConsulta7"
                android:text="@string/obesidad" />

            <ImageView
                android:id="@+id/adelante7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/icons8_adelante_24" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".12"
            android:layout_marginBottom="@dimen/marginbottom_menu"
            android:clickable="true"
            android:focusable="true"
            android:background="@drawable/ripple_effect"
            android:onClick="goEmbarazo"
            android:orientation="horizontal"
            android:paddingHorizontal="@dimen/paddinghorizontal_menu"
            android:paddingVertical="@dimen/paddingvertical_menu">

            <ImageView
                android:id="@+id/iconSoun8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/padding_buttom"
                android:layout_toStartOf="@id/adelante8"
                android:contentDescription="@string/embarazo"
                app:srcCompat="@drawable/icons8_altavoz_24"
                android:onClick="sound"/>

            <ImageView
                android:id="@+id/imageViewConsulta8"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/iconoembarazo1" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/paddingViews"
                android:layout_toEndOf="@+id/imageViewConsulta8"
                android:text="@string/embarazo" />

            <ImageView
                android:id="@+id/adelante8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/icons8_adelante_24" />
        </RelativeLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>

</android.support.design.widget.CoordinatorLayout>
