                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/icons8_adelante_24" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".12"
            android:layout_marginBottom="@dimen/marginbottom_menu"
            android:clickable="true"
            android:focusable="true"
            android:background="@drawable/ripple_effect"
            android:onClick="goHipertension"
            android:orientation="horizontal"
            android:paddingHorizontal="@dimen/paddinghorizontal_menu"
            android:paddingVertical="@dimen/paddingvertical_menu">

            <ImageView
                android:id="@+id/iconSound5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/padding_buttom"
                android:layout_toStartOf="@id/adelante5"
